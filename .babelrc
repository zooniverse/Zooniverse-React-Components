{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "env": {
    "es6": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
          "targets": {
            "esmodules": true
          }
        }],
        ["@babel/preset-react", {
          "runtime": "automatic"
        }]
      ]
    }
  }
}