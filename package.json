{
  "name": "zooniverse-react-components",
  "version": "0.9.0",
  "description": "Zooniverse-React-Components ===========================",
  "main": "lib/index.js",
  "scripts": {
    "start": "echo 'Nothing to see here yet'",
    "build": "rm -rf lib && npm run build-js && npm run build-css",
    "build-js": "babel src --out-dir lib",
    "build-css": "stylus src/css/zooniverse-react-components.styl --out lib",
    "test": "mocha --reporter nyan --require babel-core/register test/setup.js test/*.test.js",
    "lint": "eslint src",
    "preversion": "npm test",
    "version": "npm run build && git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zooniverse/Zooniverse-React-Components.git"
  },
  "author": "Zooniverse",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zooniverse/Zooniverse-React-Components/issues"
  },
  "homepage": "https://github.com/zooniverse/Zooniverse-React-Components",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "~1.9.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jsdom": "^11.1.0",
    "mocha": "^6.1.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sinon": "^1.17.6",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "animated-scrollto": "^1.1.0",
    "data-uri-to-blob": "0.0.4",
    "grommet": "^1.8.2",
    "markdownz": "^7.6.5",
    "modal-form": "^2.9.0",
    "panoptes-client": "^3.0.0-rc.0",
    "prop-types": "~15.5.10",
    "react-select": "1.0.0-rc.10",
    "react-swipe": "^6.0.4"
